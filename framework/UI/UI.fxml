<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>

<fx:root alignment="CENTER" minHeight="-Infinity" minWidth="-Infinity" type="VBox" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
    <TitledPane alignment="TOP_LEFT" animated="false" expanded="false" text="Setup">
         <content>
            <HBox fx:id="HB_Setup" prefHeight="100.0" prefWidth="200.0" />
         </content>
      </TitledPane>
      <Separator minHeight="-Infinity" />
    <TitledPane fx:id="TP_BreakHandler" animated="false" expanded="false" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Break Handler" />
      <Separator minHeight="-Infinity" />
      <TitledPane fx:id="TP_SkillTracker" animated="false" expanded="false" text="Skill Tracker" />
      <Separator minHeight="-Infinity" />
      <TitledPane fx:id="TP_ItemTracker" animated="false" expanded="false" text="Item Tracker" />
      <Separator minHeight="-Infinity" />
      <TitledPane fx:id="TP_Currently" animated="false" expanded="false" text="Currently: ">
         <content>
            <ListView fx:id="LV_CurrentTask" prefHeight="200.0" prefWidth="200.0" />
         </content>
      </TitledPane>
      <Separator prefWidth="200.0" />
      <HBox alignment="CENTER" spacing="5.0">
         <children>
            <Label fx:id="LL_Runtime" text="Runtime:">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
            <Separator orientation="VERTICAL" />
            <Label text="Stop At">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
            <TextField fx:id="TF_StopTime" alignment="CENTER" prefHeight="37.0" prefWidth="91.0" text="00:00:00">
               <font>
                  <Font size="16.0" />
               </font>
            </TextField>
         </children>
      </HBox>
      <HBox alignment="CENTER" minHeight="-Infinity" minWidth="-Infinity" prefHeight="70.0" VBox.vgrow="ALWAYS">
         <children>
            <Label fx:id="LL_BotName" alignment="CENTER" prefWidth="200.0" text="Insert Bot Name Here" textAlignment="CENTER" HBox.hgrow="ALWAYS" />
            <Separator orientation="VERTICAL" prefHeight="200.0" HBox.hgrow="ALWAYS" />
            <Label fx:id="LL_Version" alignment="CENTER" prefWidth="200.0" text="Insert Version Number Here" textAlignment="CENTER" wrapText="true" HBox.hgrow="ALWAYS" />
            <Separator orientation="VERTICAL" prefHeight="200.0" HBox.hgrow="ALWAYS" />
            <Label alignment="CENTER" prefWidth="200.0" text="Created By: Awesome123man" textAlignment="CENTER" textOverrun="CLIP" wrapText="true" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
   </children>
</fx:root>
